      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C868D1E5-5117-4DA8-9678-0181B6EFCA02}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="DECLARE @StartDate date = ?;&#xA;DECLARE @EndDate   date = ?;&#xA;&#xA;SET LANGUAGE Spanish;&#xA;SET DATEFIRST 1;&#xA;&#xA;;WITH Tally AS (&#xA;  SELECT TOP (DATEDIFF(DAY, @StartDate, @EndDate) + 1)&#xA;         ROW_NUMBER() OVER (ORDER BY (SELECT NULL)) - 1 AS n&#xA;  FROM sys.all_objects a CROSS JOIN sys.all_objects b&#xA;),&#xA;Dates AS (&#xA;  SELECT DATEADD(DAY, n, @StartDate) AS d FROM Tally&#xA;),&#xA;Src AS (&#xA;  SELECT&#xA;    YEAR(d)*10000 + MONTH(d)*100 + DAY(d)   AS tiempo_id,&#xA;    DAY(d)                                  AS dia,&#xA;    MONTH(d)                                AS mes,&#xA;    YEAR(d)                                 AS anio,&#xA;    d                                       AS fecha_completa,&#xA;    DATENAME(WEEKDAY,d)                     AS nombre_dia,&#xA;    DATENAME(MONTH,d)                       AS nombre_mes,&#xA;    LEFT(DATENAME(MONTH,d),3)               AS nombre_mes_corto,&#xA;    DATEPART(ISO_WEEK,d)                    AS semana_iso,&#xA;    DATEPART(QUARTER,d)                     AS trimestre,&#xA;    CASE WHEN DATEPART(WEEKDAY,d) IN (6,7) THEN 1 ELSE 0 END AS es_fin_de_semana&#xA;  FROM Dates&#xA;)&#xA;MERGE dbo.DIM_TIEMPO AS T&#xA;USING Src AS S&#xA;  ON T.tiempo_id = S.tiempo_id&#xA;WHEN MATCHED THEN&#xA;  UPDATE SET&#xA;    T.dia               = S.dia,&#xA;    T.mes               = S.mes,&#xA;    T.anio              = S.anio,&#xA;    T.fecha_completa    = S.fecha_completa,&#xA;    T.nombre_dia        = S.nombre_dia,&#xA;    T.nombre_mes        = S.nombre_mes,&#xA;    T.nombre_mes_corto  = S.nombre_mes_corto,&#xA;    T.semana_iso        = S.semana_iso,&#xA;    T.trimestre         = S.trimestre,&#xA;    T.es_fin_de_semana  = S.es_fin_de_semana&#xA;WHEN NOT MATCHED THEN&#xA;  INSERT (tiempo_id, dia, mes, anio, fecha_completa, nombre_dia, nombre_mes, nombre_mes_corto, semana_iso, trimestre, es_fin_de_semana)&#xA;  VALUES (S.tiempo_id, S.dia, S.mes, S.anio, S.fecha_completa, S.nombre_dia, S.nombre_mes, S.nombre_mes_corto, S.semana_iso, S.trimestre, S.es_fin_de_semana);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
